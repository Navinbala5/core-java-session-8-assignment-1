package com.example.mypackage;

public class TwentyeighthAssign extends Thread {

		public void run(){
		super.run();
		try {
			for(int i=0;i<10;i++){
			Thread.sleep(5000);
					
				System.out.println("countdown"+i);
			}
			}
		 catch (InterruptedException e) {
			
			e.printStackTrace();
		}
		
	}

}
